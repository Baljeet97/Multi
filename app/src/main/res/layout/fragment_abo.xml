<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LegalFragment">

    <Button
        android:id="@+id/tica"
        android:layout_width="235dp"
        android:layout_height="53dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="78dp"
        android:background="@drawable/rounder_corners"
        android:text="Abstudy"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/tenancy"
        android:layout_width="235dp"
        android:layout_height="84dp"

        android:layout_marginStart="78dp"
        android:layout_marginTop="60dp"

        android:background="@drawable/rounder_corners"
        android:text="Youth Allowance"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tica"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/atsils"
        android:layout_width="237dp"
        android:layout_height="64dp"

        android:layout_marginStart="78dp"
        android:layout_marginTop="68dp"


        android:background="@drawable/rounder_corners"
        android:text="Payment And Service Finder"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tenancy"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>