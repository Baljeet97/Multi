        app:layout_constraintBottom_toTopOf="@+id/homeless"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/qldHousing"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/homeless"
        android:layout_width="252dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/rounder_corners"


        android:text="Link to Govenment Help Sites"
        android:visibility="invisible"
        app:layout_constraintTop_toBottomOf="@+id/legallyYours"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="81dp" />
</androidx.constraintlayout.widget.ConstraintLayout>