        app:layout_constraintTop_toBottomOf="@+id/bond"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/legalAid"
        android:layout_width="252dp"
        android:layout_height="wrap_content"

        android:layout_marginBottom="10dp"
